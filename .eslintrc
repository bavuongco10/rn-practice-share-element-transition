{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "react-native/react-native": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react-native/no-unused-styles": 2,
    "react-native/no-inline-styles": 2
  },
  "ecmaFeatures": {
    "jsx": true
  }
}